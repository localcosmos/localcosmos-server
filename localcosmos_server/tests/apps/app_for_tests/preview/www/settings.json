                    ]
                },
                "help_text": "Will be displayed in the funding partner sections",
                "required": false
            },
            "funding_partner_3": {
                "restrictions": {
                    "file_type": [
                        "jpg",
                        "jpeg",
                        "png"
                    ]
                },
