                "required": false
            },
            "fundingPartner3": {
                "restrictions": {
                    "fileType": [
                        "jpg",
                        "jpeg",
                        "png"
                    ]
                },
                "helpText": "Will be displayed in the funding partner sections",
                "required": false
            }
        },
        "texts": {
