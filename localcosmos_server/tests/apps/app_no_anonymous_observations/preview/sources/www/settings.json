            },
            "appPcBackground": {
                "restrictions": {
                    "fileType": [
                        "jpg",
                        "jpeg",
                        "png"
                    ],
                    "ratio": "2:1"
                },
                "helpText": "Will be displayed in the webapp using a pc",
                "required": false
            },
            "appSplashscreen": {
                "restrictions": {
                    "fileType": [
                        "svg"
                    ],
                    "ratio": "1:1"
                },
                "helpText": "Will be displayed during app startup",
                "required": true
            },
            "logo": {
                "restrictions": {
                    "fileType": [
                        "svg"
                    ]
                },
                "helpText": "Will be displayed on the apps main page",
